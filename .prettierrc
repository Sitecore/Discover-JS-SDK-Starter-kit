{
  "printWidth": 120,
  "trailingComma": "all",
  "semi": true,
  "singleQuote": true,
  "quoteProps": "consistent",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "tabWidth": 2,
  "parser": "typescript",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
